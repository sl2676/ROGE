	setSrcRegIdx(_numSrcRegs++, RegId(VecRegClass, VECREG_UREG0));
	setDestRegIdx(_numDestRegs++, RegId(VecPredRegClass, PREDREG_UREG0));
	_numVecPredDestRegs++;
	setSrcRegIdx(_numSrcRegs++, RegId(VecPredRegClass, gp));
	flags[IsLoad] = true;
	flags[IsMicroop] = true;
	flags[IsVector] = true;;
            if (_opClass == MemReadOp && elemIndex == 0) {
