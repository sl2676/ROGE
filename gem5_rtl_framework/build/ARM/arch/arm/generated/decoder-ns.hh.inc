                // destination and the fault status registers
                assert(_numDestRegs == 2);
                for (int i = 0; i < _numDestRegs; i++) {
                    auto dr = destRegIdx(i);
                    dr.setNumPinnedWrites(numElems - 1);
                    setDestRegIdx(i, dr);
                }
            }
        }

