                        machInst, zd, zn, zm);
            }
        } else if (bits(machInst, 23, 22) == 0x0 &&
                bits(machInst, 20, 17) == 0x8 && bits(machInst, 12, 9) == 0x0
                && bits(machInst, 4) == 0x0) {
            IntRegIndex pd = (IntRegIndex) (uint8_t) bits(machInst, 3, 0);
            IntRegIndex pn = (IntRegIndex) (uint8_t) bits(machInst, 8, 5);
            if (bits(machInst, 16)) {
